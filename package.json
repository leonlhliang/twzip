{
    "devDependencies": {
        "supertest-as-promised": "1.0.0",
        "grunt-contrib-jshint": "0.10.0",
        "chai-as-promised": "4.1.1",
        "recursive-readdir": "1.2.0",
        "precommit-hook": "latest",
        "expect.js": "0.3.1",
        "coveralls": "2.11.2",
        "bluebird": "2.3.11",
        "cucumber": "0.4.4",
        "istanbul": "0.3.4",
        "grunt": "0.4.5",
        "chai": "1.10.0"
    },
    "dependencies": {
        "express": "4.10.4",
        "morgan": "1.5.0"
    },
    "description": "Get 5-digit zipcode for Taiwanese address",
    "repository": "https://github.com/lihengl/dizu-api.git",
    "precommit": [
        "lint",
        "test-local"
    ],
    "version": "0.1.0",
    "scripts": {
        "compile": "bin/parse.py src lib",
        "bundle": "rm -r src bin features coverage node_modules && zip -r bundle.zip . -x *.git*",
        "test-local": "istanbul cover cucumber.js",
        "test": "istanbul cover cucumber.js --report lcovonly && cat coverage/lcov.info | coveralls",
        "lint": "grunt jshint && travis-lint"
    },
    "author": "Li-Heng Liang <leonlhliang@gmail.com>",
    "main": "server.js",
    "name": "dizu-api"
}
