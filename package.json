{
    "devDependencies": {
        "chai-as-promised": "4.1.1",
        "precommit-hook": "latest",
        "cucumber-junit": "1.1.9",
        "expect.js": "0.3.1",
        "coveralls": "2.11.2",
        "bluebird": "2.3.11",
        "cucumber": "0.4.4",
        "istanbul": "0.3.4",
        "chai": "1.10.0"
    },
    "dependencies": {
        "express": "4.10.4",
        "morgan": "1.5.0"
    },
    "description": "Get 5-digit zipcode for Taiwanese address",
    "repository": "https://github.com/lihengl/dizu-api.git",
    "precommit": [
        "test-local"
    ],
    "version": "0.1.0",
    "scripts": {
        "test-local": "cucumber-js feature/local.feature --require test --format pretty",
        "test-unit": "cucumber-js feature/unit.feature --require test --format json | cucumber-junit > artifact/report.xml",
        "compile": "bin/parse.py doc lib",
        "cover": "istanbul cover test/* --report lcovonly && cat ./coverage/lcov.info | coveralls",
        "test": "cucumber-js feature/functional.feature --require test --format json | cucumber-junit > artifact/report.xml",
        "lint": "grunt jslint"
    },
    "author": "lihengl",
    "main": "server.js",
    "name": "dizu-api"
}
