{
    "devDependencies": {
        "chai-as-promised": "4.1.1",
        "precommit-hook": "latest",
        "expect.js": "0.3.1",
        "supertest": "0.15.0",
        "coveralls": "2.11.2",
        "bluebird": "2.3.11",
        "cucumber": "0.4.4",
        "istanbul": "0.3.4",
        "chai": "1.10.0"
    },
    "dependencies": {
        "express": "4.10.4",
        "morgan": "1.5.0"
    },
    "description": "Get 5-digit zipcode for Taiwanese address",
    "repository": "https://github.com/lihengl/dizu-api.git",
    "precommit": [
        "test-smoke"
    ],
    "version": "0.1.0",
    "scripts": {
        "cover": "istanbul cover test/api.js --report lcovonly && cat ./coverage/lcov.info | coveralls",
        "test-smoke": "cucumber-js feature/smoke.feature --require test --format pretty",
        "test-unit": "cucumber-js feature/unit.feature --require test --format summary",
        "test": "cucumber-js feature/functional.feature --require test --format pretty",
        "compile": "bin/parse.py doc lib",
        "lint": "grunt jslint && travis-lint"
    },
    "author": "Li-Heng Liang <leonlhliang@gmail.com>",
    "main": "server.js",
    "name": "dizu-api"
}
